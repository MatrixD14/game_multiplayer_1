 //[I-S]LN=77;[I-E]
      if (neighbors.size() > 0) { //[I-S]LN=78;[I-E]
        float noises = noise.noise(currentCell + seed + k, 0); //[I-S]LN=79;[I-E]
        int idx = (int) (((noises + 1) * .5f) * neighbors.size()); //[I-S]LN=80;[I-E]
        if (idx >= neighbors.size()) idx = neighbors.size() - 1; //[I-S]LN=81;[I-E]
        if (idx < 0) idx = 0; //[I-S]LN=82;[I-E]
        if (neighbors.size() == 0) { //[I-S]LN=83;[I-E]
          if (path.size() == 0) break; //[I-S]LN=84;[I-E]
