        } else { //[I-S]LN=86;[I-E]
          path.push(currentCell); //[I-S]LN=87;[I-E]
          int newCell = neighbors.get(idx); //[I-S]LN=88;[I-E]
          if (newCell > currentCell) { //[I-S]LN=89;[I-E]
            if (newCell - 1 == currentCell) { //[I-S]LN=90;[I-E]
              board.get(currentCell).status[3] = 1; //[I-S]LN=91;[I-E]
              currentCell = newCell; //[I-S]LN=92;[I-E]
              board.get(currentCell).status[1] = 1; //[I-S]LN=93;[I-E]
            } else { //[I-S]LN=94;[I-E]
              board.get(currentCell).status[2] = 1; //[I-S]LN=95;[I-E]
              currentCell = newCell; //[I-S]LN=96;[I-E]
              board.get(currentCell).status[0] = 1; //[I-S]LN=97;[I-E]
            } //[I-S]LN=98;[I-E]
          } else { //[I-S]LN=99;[I-E]
            if (newCell + 1 == currentCell) { //[I-S]LN=100;[I-E]
              board.get(currentCell).status[1] = 1; //[I-S]LN=101;[I-E]
              currentCell = newCell; //[I-S]LN=102;[I-E]
              board.get(currentCell).status[3] = 1; //[I-S]LN=103;[I-E]
            } else { //[I-S]LN=104;[I-E]
