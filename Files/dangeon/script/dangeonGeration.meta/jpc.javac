 //[I-S]LN=115;[I-E]
  public List<Integer> checkNeighbors(int cell) { //[I-S]LN=116;[I-E]
    List<Integer> neighbors = new LinkedList<Integer>(); //[I-S]LN=117;[I-E]
 //[I-S]LN=118;[I-E]
    if ((cell - size.x) > 0 && !board.get(cell - size.x).vision) neighbors.add(cell - size.x); //[I-S]LN=119;[I-E]
 //[I-S]LN=120;[I-E]
    if ((cell + size.x) < board.size() && !board.get(cell + size.x).vision) neighbors.add(cell + size.x); //[I-S]LN=121;[I-E]
 //[I-S]LN=122;[I-E]
    if (((cell + 1) % size.x) != 0 && !board.get(cell + 1).vision) neighbors.add(cell + 1); //[I-S]LN=123;[I-E]
 //[I-S]LN=124;[I-E]
